generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
}


model auths {
  id          Int      @id @default(autoincrement())
  accessToken String
  createdAt   DateTime @db.Date
  email       String
  name        String
  updatedAt   DateTime @db.Date
}

model contacts {
  id              Int                  @id @default(autoincrement())
  createdAt       DateTime             @db.Date
  dob             DateTime             @db.Date
  gender          String?
  last_fetched    DateTime?            
  mobile_number   String               @unique(map: "mobile_number_1")
  name            String?
  series_number   Int
  updatedAt       DateTime             @db.Date
  whatsapp_number String
  pob_description String?
  pob_latitude Float?
  pob_longitude Float?
}

model ivrs {
  id             Int      @id @default(autoincrement())
  agent_duration String?
  agent_name     String?
  agent_number   String?
  circle         String?
  createdAt      DateTime @db.Date
  extension      String?
  from           String?
  operator       String?
  recording      String?
  status         String?
  time           String?
  to             String?
  total_duration String?
  uniqueid       String?
  unix           String?
  updatedAt      DateTime @db.Date
}
